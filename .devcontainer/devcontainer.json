{
    "name": "Rustlind",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "rust-lang.rust-analyzer"
            ]
        }
    },
    "runArgs": [
        "--env-file",
        "${localWorkspaceFolder}/.devcontainer/.env"
    ]
}